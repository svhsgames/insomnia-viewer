{
	"$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"lib": [ "es2020", "dom" ],
		"module": "commonjs",
		"target": "es5",
		"baseUrl": "ts",
		"types": [ "node", "react", "webpack", "jquery", "webpack-env" ],
		"jsx": "react",
		"outDir": "__compile_cache",
		"sourceMap": true,
		"strict": true,
		"allowJs": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noUnusedParameters": true,
		"allowUnreachableCode": false,
		"preserveConstEnums": false,
		"resolveJsonModule": true
	}
}